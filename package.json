{
  "name": "weather-for-humans",
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "start": "NODE_ENV=production node ./app.js",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "local:generate:cert": "openssl req -x509 -newkey rsa:2048 -nodes -sha256 -subj '/CN=localhost' -keyout key.pem -out cert.pem",
    "lint:check": "eslint .",
    "lint:fix": "eslint . --fix",
    "dev": "NODE_ENV=development node ./app.js"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "./services/**",
      "./routes/**"
    ],
    "coverageThreshold": {
      "global": {
        "lines": 90
      }
    }
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^16.0.1",
    "express": "~4.16.1",
    "express-handlebars": "^6.0.6",
    "geoip-lite": "^1.4.5",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1",
    "node-fetch": "^3.2.4"
  },
  "devDependencies": {
    "@types/jest": "^28.1.2",
    "eslint": "^8.20.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.5.3",
    "eslint-plugin-n": "^15.2.4",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "jest": "^28.1.1"
  }
}
